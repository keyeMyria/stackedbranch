    "core-js": "^2.5.1",
    "crossfilter2": "^1.4.3",
    "d3-transition": "^1.1.1",
    "dc": "2.1.9",
    "dc-react": "0.0.3",
    "dc.leaflet": "^0.3.3",
    "dom-to-image": "^2.6.0",
    "dva": "^2.1.0",
    "file-saver": "^1.3.3",
    "freezer-js": "^0.11.2",
    "fs": "0.0.1-security",
    "highcharts": "^6.0.4",
    "highcharts-funnel": "^0.1.7",
    "highcharts-more": "^0.1.7",
    "html2canvas": "^1.0.0-alpha.9",
    "jquery": "^3.3.1",
    "keymaster": "^1.6.2",
    "leaflet": "^1.2.0",
    "leaflet-d3-svg-overlay": "^2.2.0",
    "leaflet-draw": "^0.4.13",
    "leaflet.heat": "^0.2.0",
    "leaflet.markercluster": "^1.2.0",
    "less-vars-to-js": "^1.2.1",
    "lodash": "^4.17.4",
    "lodash-decorators": "^4.4.1",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.19.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.5.10",
    "qs": "^6.5.0",
    "react": "^16.2.0",
    "react-addons-transition-group": "^15.6.2",
    "react-animate-height": "^0.10.9",
    "react-container-query": "^0.9.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.2.0",
    "react-fittext": "^1.0.0",
